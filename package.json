  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "cross-env": "^5.2.0",
    "dmd": "^4.0.4",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^6.1.3",
    "nyc": "^14.1.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  }
