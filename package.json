    "build:umd:min": "cross-env MODE=production webpack",
    "build:doc": "node scripts/build-doc.js",
    "demo": ""
  },
  "keywords": [
    "shadow-fns",
    "functional"
  ],
  "files": [
    "dist",
    "lib",
    "src"
  ],
