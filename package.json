{
  "name": "shadow-fns",
  "version": "0.1.0",
  "description": "A javascript function library.",
  "main": "./lib/index.js",
  "repository": "",
  "scripts": {
    "test": "cross-env mocha --require @babel/register",
    "cover": "yarn cover:unit && yarn cover:report",
